{
  "name": "dla",
  "version": "0.0.1",
  "description": "node.js data loader with caching and support of lists",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage dist",
    "build": "tsc",
    "test": "nyc mocha dist/tests",
    "lint": "tslint src/**/*",
    "coverals": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run lint && npm run clean && npm run build && npm test"
  },
  "keywords": [
    "data",
    "loader"
  ],
  "author": "Roman Ditchuk",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "2.2.44",
    "@types/node": "8.0.49",
    "coveralls": "3.0.0",
    "mocha": "4.0.1",
    "nyc": "11.3.0",
    "source-map-support": "0.5.0",
    "tslint": "5.8.0",
    "typescript": "2.6.1"
  },
  "dependencies": {
    "object-hash": "1.2.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "**/*"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false
  }
}
